{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "~/*": [
        "*"
      ],
    },
    "baseUrl": "./src",
    "module": "node16",
    "moduleResolution": "node16"
  }
}